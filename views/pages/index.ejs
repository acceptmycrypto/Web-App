<head>
  <% include ../partials/head %>
</head>

<body>
  <header>
    <% include ../partials/header %>
  </header>
  <main>


    <div class="limiter">
      <div class="container-table100">
        <div class="wrap-table100">
          <div class="table">

            <div class="row header">
              <div class="cell">
                Crypto Name
              </div>
              <div class="cell">
                Rank
              </div>
              <div class="cell">
                # of Venues Accepting
              </div>
              <div class="cell">
                Price
              </div>
            </div>

            <% cryptos.forEach(function(data) { %>
              <div class="row">
                <div class="cell" data-title="crypto">
                  <a href="/cryptos/<%= data.crypto_name %>">
                    <%= data.crypto_name %></a>
                </div>
                <div class="cell" data-title="rank">
                  Rank (TBD)
                </div>
                <div class="cell" data-title="vendors">
                  # of Vendors Accepting (TBD)
                  
                 
                </div>
                <div class="cell" data-title="price">
                  $ <%= data.crypto_price %>
                </div>
              </div>
              <% }); %>


          </div>
        </div>
      </div>
    </div>

  </main>

  <footer>
    <% include ../partials/footer %>
  </footer>


  <script src="/assets/vendor/bootstrap/js/bootstrap.min.js"></script>
  <script src="/assets/javascript/app.js"></script>
  <script src="/assets/javascript/nav.js"></script>

</body>

